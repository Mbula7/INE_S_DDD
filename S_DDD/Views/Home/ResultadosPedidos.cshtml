
@{
    var totalPendentes = ViewBag.TotalPendentes ?? 0;
    var totalRespondidos = ViewBag.TotalRespondidos ?? 0;
}


<div class="card shadow p-3 mb-4">
    <h3 class="text-center mb-4" style="font-size: 16px;">RESULTADOS ENCONTRADOS</h3>
    <table class="table table-bordered">
        <tbody>
            <tr>
                <td><strong>Total de Registos Pendentes</strong></td>
                <td>
                    <a href="#">@totalPendentes</a>
                </td>
            </tr>
            <tr>
                <td><strong>Total de Registo Respondidos</strong></td>
                <td>
                    <a href="#">@totalRespondidos</a>
                </td>
            </tr>
        </tbody>
    </table>
</div>


